<div id="selection">
  <div class="container">
    <div class="row">
      <div class="row">
        <div
          v-for="(glass, index) in glasses"
          :key="index"
          class="col-lg-3 col-md-6 mb-3"
        >
          <item-adder
            :product="glass"
            :index="index"
            :handle-submitting="handleItemSubmitting"
            :syncing.sync="syncing"
            :cloud-error.sync="cloudError"
            />
        </div>
      </div>
    </div>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>